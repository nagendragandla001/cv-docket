{
  "name": "cv-docket",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node server.js",
    "gcp-predeploy": "find ./build -name '\\[...*' -exec bash -c 'mv \"$1\" \"${1/.../@@@}\"' -- {} \\;",
    "gcp-build": "find ./build -name '\\[@@@*' -exec bash -c 'mv \"$1\" \"${1/@@@/...}\"' -- {} \\;",
    "build": "next build",
    "start": "next start -p 8080",
    "deploy": "npm run build && gcloud app deploy",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@ant-design/icons": "^4.2.2",
    "@zeit/next-less": "^1.0.1",
    "antd": "^4.8.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "less": "^3.12.2",
    "less-vars-to-js": "^1.3.0",
    "moment": "^2.29.1",
    "next": "9.5.3",
    "next-compose-plugins": "^2.2.0",
    "next-routes": "^1.4.2",
    "react": "16.13.1",
    "react-device-detect": "^1.14.0",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "@types/node": "^14.11.2",
    "@types/react": "^16.9.50",
    "less-loader": "^7.0.1",
    "typescript": "^4.0.3"
  }
}
